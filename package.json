{
   "name": "graphql-ts-server",
   "version": "0.0.1",
   "description": "Boilerplate server made with Typescript, Postgres and Redis",
   "author": "iamasteriix",
   "type": "commonjs",
   "scripts": {
      "start": "NODE_ENV=dev ts-node src/index.ts",
      "test": "NODE_ENV=test jest --config jest.config.ts --detectOpenHandles",
      "generate-types": "graphql-codegen --config codegen.yml",
      "typeorm": "typeorm-ts-node-commonjs",
      "lint": "eslint -c .eslintrc.js --ext .ts src"
   },
   "devDependencies": {
      "@graphql-codegen/cli": "2.6.2",
      "@graphql-codegen/typescript": "2.4.11",
      "@graphql-codegen/typescript-document-nodes": "2.2.11",
      "@graphql-codegen/typescript-resolvers": "2.6.4",
      "@types/bcryptjs": "^2.4.2",
      "@types/ioredis": "^4.28.10",
      "@types/jest": "^27.5.1",
      "@types/node": "^16.11.10",
      "@types/uuid": "^8.3.4",
      "@types/yup": "^0.29.14",
      "@typescript-eslint/eslint-plugin": "^5.25.0",
      "@typescript-eslint/parser": "^5.25.0",
      "eslint": "^8.15.0",
      "graphql-request": "^4.2.0",
      "jest": "^28.1.0",
      "nodemon": "^2.0.16",
      "ts-jest": "^28.0.2",
      "ts-node": "10.7.0",
      "typescript": "4.5.2"
   },
   "dependencies": {
      "@graphql-tools/graphql-file-loader": "^7.3.12",
      "@graphql-tools/load": "^7.5.11",
      "@graphql-tools/schema": "^8.3.13",
      "apollo-server": "^3.7.0",
      "bcryptjs": "^2.4.3",
      "graphql": "^16.5.0",
      "ioredis": "^4.28.5",
      "pg": "^8.4.0",
      "reflect-metadata": "^0.1.13",
      "typeorm": "0.3.6",
      "uuid": "^8.3.2",
      "yup": "^0.32.11"
   }
}
